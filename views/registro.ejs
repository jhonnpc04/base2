<h2>registroReclamo</h2>
<form class="nav-var" action="guardarReclamo" method="post">
<label for="tiporeclamo">Tiporeclamo</label>
<select class="list" name="tiporeclamo" id="tiporeclamo">
  <option value="option">Odeco</option>
  <option value="option1">option1</option>
  <option value="option2">option2</option>
  <option value="option3">option3</option>
</select>
<div class="nav-var">
  <label for="observaciones">Observaciones: </label>
  <input type="text" id="observacion" name="reclamo[observacion]" value="<%= reclamo.observacion %>">
</div>
<div class="nav-var">
  <label for="fecha">Fecha</label>
  <input type="text" id="fechareclamo" name="reclamo[fechareclamo]" value="<%= reclamo.fechareclamo %>">
</div>
<div class="nav-var">
  <input type="submit" name="envio" value="Registrar Reclamo">
</div>
</form>
